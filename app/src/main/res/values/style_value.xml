<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Circular">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="RoundedSquare">
        <item name="cornerSize">30%</item>
    </style>

    <style name="CoutnerCut">
        <item name="cornerSize">50dp</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="DiamondCut">
        <item name="cornerSize">150dp</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="SpasificCournerCut">
        <item name="cornerSizeTopLeft">50dp</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerSizeBottomRight">50dp</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="SpasificCournerCutRounded">
        <item name="cornerSizeBottomLeft">160dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
    </style>

    <style name="MyButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="shapeAppearanceOverlay">@style/MyShapeAppearance</item>
    </style>
    <style name="MyShapeAppearance">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">30dp</item>
    </style>




    <style name="SpasificCournerCutRoundeds">
        <item name="cornerSizeTopRight">80dp</item>
        <item name="cornerFamilyTopRight">rounded</item>
    </style>

    <style name="CustomCardViewStyle" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_custom_corners</item>
    </style>


    <style name="ShapeAppearanceOverlay_card_custom_corners" parent="">

        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">50dp</item>
        <item name="cornerSizeBottomLeft">50dp</item>
    </style>



    <style name="CustomCardViewStyles" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_custom_cornerss</item>
    </style>


    <style name="ShapeAppearanceOverlay_card_custom_cornerss" parent="">

        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">18dp</item>
        <item name="cornerSizeTopLeft">18dp</item>
        <item name="cornerSizeBottomRight">18dp</item>
        <item name="cornerSizeBottomLeft">2dp</item>
    </style>


    <style name="CustomCardViewStyleschat" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_custom_cornerschat</item>
    </style>


    <style name="ShapeAppearanceOverlay_card_custom_cornerschat" parent="">

        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">18dp</item>
        <item name="cornerSizeTopLeft">2dp</item>
        <item name="cornerSizeBottomRight">18dp</item>
        <item name="cornerSizeBottomLeft">18dp</item>
    </style>


    <style name="CustomCardViewStyleschatuser" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_custom_cornerschatuser</item>
    </style>


    <style name="ShapeAppearanceOverlay_card_custom_cornerschatuser" parent="">

        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">18dp</item>
        <item name="cornerSizeTopLeft">18dp</item>
        <item name="cornerSizeBottomRight">2dp</item>
        <item name="cornerSizeBottomLeft">18dp</item>
    </style>



    <style name="SpasificCournerCuthaf">
        <item name="cornerSizeTopLeft">50dp</item>
        <item name="cornerSizeBottomLeft">50dp</item>
        <item name="cornerSizeBottomRight">2dp</item>
        <item name="cornerSizeTopRight">2dp</item>
    </style>

    <style name="SpasificCournerCuthafs">
        <item name="cornerSizeTopLeft">2dp</item>
        <item name="cornerSizeBottomLeft">2dp</item>
        <item name="cornerSizeBottomRight">50dp</item>
        <item name="cornerSizeTopRight">50dp</item>
    </style>

    <style name="Widget.MaterialComponents.CardView_" parent="Widget.MaterialComponents.CardView">
        <item name="android:background">@drawable/ig_gt</item>
    </style>



